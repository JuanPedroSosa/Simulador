<!DOCTYPE html>
<html>

<body>
	<h1>Simulador estaci√≥n de inflado</h1>
    <ul>
        {{#each msg}}
        <li>{{this}}</li>
        {{/each}}
    </ul>
		<button onclick="probar()">OK</button>
</body>
<script>
	function probar() {
		alert("test");
		var myHeaders = new Headers();
		myHeaders.append("Content-Type", "application/json");

		var raw = JSON.stringify({
		  "msg": "estcion 3000, servicio 1, pos 1"
		});

		var requestOptions = {
		  method: 'POST',
		  headers: myHeaders,
		  body: raw,
		  redirect: 'follow'
		};

		fetch("http://localhost:5000/", requestOptions)
		  .then(response => response.text())
		  .then(result => console.log(result))
		  .catch(error => console.log('error', error));
			}
</script>
</html>